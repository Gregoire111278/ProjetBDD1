{%  extends 'base.html.twig'%}
{% block title %}All post{% endblock %}
    {% block body %}
        <div>
            {% if messages|length > 0 %}

                {% for key,message in messages|slice(0, limit) %}

                    <div>
                        <a href="{{ path('app_ShowOne', { id: key }) }}">{{message.message}}</a>
                    </div>
                    {{ include('hello/_message.html.twig', {message: message}) }}

                {% endfor %}
            {% else %}
                There's nothing here yet!
            {% endif %}
        </div>
    {% endblock %}